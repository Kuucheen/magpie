{
  "protocols": {
    "http": false,
    "https": true,
    "socks4": false,
    "socks5": false
  },

  "checker": {
    "dynamic_threads": true,
    "threads": 250,
    "retries": 2,
    "timeout": 7500,
    "checker_timer": {
      "days": 0,
      "hours": 5,
      "minutes": 0,
      "seconds": 0
    },

    "judges_threads": 3,
    "judges_timeout": 5000,
    "judges": [
      {
        "url": "https://pool.proxyspace.pro/judge.php",
        "regex": "default"
      },
      {
        "url": "http://azenv.net",
        "regex": "default"
      }
    ],
    "judge_timer": {
      "days": 0,
      "hours": 1,
      "minutes": 0,
      "seconds": 0
    },

    "use_https_for_socks": true,
    "ip_lookup": "https://ident.me",

    "standard_header": [
      "USER-AGENT", "HOST", "ACCEPT", "ACCEPT-ENCODING"
    ],

    "proxy_header": [
      "HTTP_X_FORWARDED_FOR", "HTTP_FORWARDED", "HTTP_VIA", "HTTP_X_PROXY_ID"
    ]
  },

  "scraper": {
    "dynamic_threads": true,
    "threads": 250,
    "retries": 2,
    "timeout": 7500,

    "scraper_timer": {
      "days": 0,
      "hours": 10,
      "minutes": 0,
      "seconds": 0
    },

    "scrape_sites": [
      "https://raw.githubusercontent.com/dpangestuw/Free-Proxy/refs/heads/main/http_proxies.txt",
      "https://raw.githubusercontent.com/dpangestuw/Free-Proxy/refs/heads/main/socks5_proxies.txt",
      "https://raw.githubusercontent.com/dpangestuw/Free-Proxy/refs/heads/main/socks4_proxies.txt"
    ]
  },

  "blacklist_sources": [
    "https://www.spamhaus.org/drop/drop.txt",
    "https://www.spamhaus.org/drop/edrop.txt",
    "http://myip.ms/files/blacklist/general/latest_blacklist.txt"
  ]
}